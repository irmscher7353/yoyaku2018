<!DOCTYPE html>
<html>
  <head>
    <title>Yoyaku2018</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
		<div class="container-fluid" >
			<div class="row" >
				<div class="col-md-2 d-none d-md-block bg-light sidebar" >
					<div class="sidebar-sticky" >
						<ul class="nav flex-column" >
							<li class="nav-item" ><%= link_to "Menus", menus_path -%></li>
							<li class="nav-item" ><%= link_to "Titles", titles_path -%></li>
							<li class="nav-item" ><%= link_to "Products", products_path -%></li>
							<li class="nav-item" ><%= link_to "Preferences", preferences_path -%></li>
						</ul>
					</div>
				</div>
				<div class="col-md-9 ml-sm-auto col-lg-10 px-4 main" >
    <%= yield %>
				</div>
			</div>
		</div>
  </body>
</html>
